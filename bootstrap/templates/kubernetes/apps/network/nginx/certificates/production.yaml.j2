---
apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: "chalk-playground-space-production"
spec:
  secretName: "chalk-playground-space-production-tls"
  issuerRef:
    name: letsencrypt-production
    kind: ClusterIssuer
  commonName: "chalk-playground.space"
  dnsNames:
    - "chalk-playground.space"
    - "*.chalk-playground.space"
